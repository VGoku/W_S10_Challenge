{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    {
      "src": "^/static/js/(.+)\\.js$",
      "headers": {
        "content-type": "application/javascript; charset=utf-8",
        "cache-control": "public, max-age=31536000, immutable",
        "x-content-type-options": "nosniff"
      },
      "dest": "/static/js/$1.js"
    },
    {
      "src": "^/static/css/(.+)\\.css$",
      "headers": {
        "content-type": "text/css; charset=utf-8",
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/css/$1.css"
    },
    {
      "src": "^/static/media/(.+)\\.(png|jpg|jpeg|gif|svg|ico|ttf|woff|woff2)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/media/$1.$2"
    },
    {
      "src": "^/manifest\\.json$",
      "headers": {
        "content-type": "application/json; charset=utf-8",
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "dest": "/manifest.json"
    },
    {
      "src": "^/favicon\\.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/robots\\.txt$",
      "dest": "/robots.txt"
    },
    {
      "src": "^/.*",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      },
      "dest": "/index.html"
    }
  ]
}