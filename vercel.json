{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/js/(.+)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript; charset=utf-8"
      },
      "dest": "/static/js/$1"
    },
    {
      "src": "/static/css/(.+)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "text/css; charset=utf-8"
      },
      "dest": "/static/css/$1"
    },
    {
      "src": "/static/media/(.+)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/media/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/((?!static|.*\\.[a-z0-9]+$).+)",
      "headers": {
        "cache-control": "public, no-cache, must-revalidate"
      },
      "dest": "/index.html"
    }
  ]
}